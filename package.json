{
  "displayName": "Plexus",
  "name": "plexus",
  "description": "AI Agent Observability Platform",
  "version": "0.1.0",
  "main": "./node_modules/.dev/main/index.mjs",
  "resources": "src/resources",
  "author": {
    "name": "Jinmu Go",
    "email": "hi@jinmu.me"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependenciesFile": "./trusted-dependencies-scripts.json"
  },
  "scripts": {
    "start": "electron-vite preview",
    "predev": "run-s clean:dev compile:hooks",
    "dev": "tsx scripts/dev-wrapper.ts",
    "compile:app": "electron-vite build",
    "compile:app:analyze": "ANALYZE=true electron-vite build",
    "compile:hooks:claude": "npx esbuild src/agent-scripts/plexus-hook.ts --bundle --platform=node --target=node22 --outfile=node_modules/.dev/hooks/plexus-hook.js",
    "compile:hooks:gemini": "npx esbuild src/agent-scripts/plexus-gemini-hook.ts --bundle --platform=node --target=node22 --outfile=node_modules/.dev/hooks/plexus-gemini-hook.js",
    "compile:hooks:cursor": "npx esbuild src/agent-scripts/plexus-cursor-hook.ts --bundle --platform=node --target=node22 --outfile=node_modules/.dev/hooks/plexus-cursor-hook.js",
    "compile:hooks": "run-p compile:hooks:claude compile:hooks:gemini compile:hooks:cursor",
    "compile:packageJSON": "tsx ./scripts/release/modules/prebuild.ts",
    "compile:all": "run-p compile:app compile:hooks",
    "prebuild": "run-s clean:dev compile:all compile:packageJSON",
    "build": "pnpm electron-builder",
    "postinstall": "run-s prebuild install:deps install:hooks",
    "install:deps": "electron-builder install-app-deps",
    "install:hooks": "tsx scripts/install-hooks.ts",
    "make:release": "tsx ./scripts/release/modules/release.ts",
    "release": "electron-builder --publish always",
    "clean:dev": "rimraf ./node_modules/.dev",
    "lint": "biome check --no-errors-on-unmatched",
    "lint:fix": "biome check --write --no-errors-on-unmatched --assist-enabled=true",
    "format": "biome format --write src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@tanstack/react-virtual": "^3.13.13",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-log": "^5.4.3",
    "electron-router-dom": "^2.1.0",
    "electron-store": "^11.0.2",
    "electron-unhandled": "^5.0.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.556.0",
    "node-pty": "^1.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "strip-ansi": "^7.1.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@electron/fuses": "^2.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "code-inspector-plugin": "^1.3.0",
    "cross-env": "^10.1.0",
    "electron": "^39.2.6",
    "electron-builder": "^26.3.6",
    "electron-vite": "^4.0.1",
    "esbuild": "^0.27.2",
    "jsdom": "^27.4.0",
    "lefthook": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "semver": "^7.7.3",
    "shadcn": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  }
}